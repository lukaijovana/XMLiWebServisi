  public RtgsPortProxy() {
    _initRtgsPortProxy();
  }
  
  public RtgsPortProxy(String endpoint) {
    _endpoint = endpoint;
    _initRtgsPortProxy();
  }
  
  private void _initRtgsPortProxy() {
    try {
      rtgsPort = (new rs.ac.uns.ftn.informatika.www.mt103.RtgsPortServiceLocator()).getRtgsPortSoap11();
      if (rtgsPort != null) {
        if (_endpoint != null)
          ((javax.xml.rpc.Stub)rtgsPort)._setProperty("javax.xml.rpc.service.endpoint.address", _endpoint);
        else
          _endpoint = (String)((javax.xml.rpc.Stub)rtgsPort)._getProperty("javax.xml.rpc.service.endpoint.address");
      }
      
    }
    catch (javax.xml.rpc.ServiceException serviceException) {}
  }
  
  public String getEndpoint() {
    return _endpoint;
  }
  
  public void setEndpoint(String endpoint) {
    _endpoint = endpoint;
    if (rtgsPort != null)
      ((javax.xml.rpc.Stub)rtgsPort)._setProperty("javax.xml.rpc.service.endpoint.address", _endpoint);
    
  }
  
  public rs.ac.uns.ftn.informatika.www.mt103.RtgsPort getRtgsPort() {
    if (rtgsPort == null)
      _initRtgsPortProxy();
    return rtgsPort;
  }
  
  public rs.ac.uns.ftn.informatika.www.mt103.SendRtgsResponse sendRtgs(rs.ac.uns.ftn.informatika.www.mt103.SendRtgsRequest sendRtgsRequest) throws java.rmi.RemoteException{
