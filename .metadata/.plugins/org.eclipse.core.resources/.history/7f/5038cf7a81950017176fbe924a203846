package rs.ac.uns.ftn.informatika.rest.service;

import java.util.List;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;

import rs.ac.uns.ftn.informatika.rest.domain.Izvod;
import rs.ac.uns.ftn.informatika.rest.repository.IzvodRepository;

@Service
public class IzvodServiceImpl implements IzvodService {
	
	@Autowired
	IzvodRepository izvodRepository;

	@Override
	public List<Izvod> findAll() {
		// TODO Auto-generated method stub
		return izvodRepository.findAll();
	}

	@Override
	public Izvod findById(Long id) {
		// TODO Auto-generated method stub
		return izvodRepository.findById(id);
	}

	@Override
	public void delete(Long id) {
		// TODO Auto-generated method stub
		izvodRepository.delete(id);
	}

	@Override
	public Izvod save(Izvod izvod) {
		// TODO Auto-generated method stub
		return izvodRepository.save(izvod);
	}

}
